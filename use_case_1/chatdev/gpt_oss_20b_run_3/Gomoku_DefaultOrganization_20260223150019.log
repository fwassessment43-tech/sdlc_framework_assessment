            restart_rect = restart_msg.get_rect(center=(SCREEN_SIZE // 2, SCREEN_SIZE // 2 + 20))
            self.screen.blit(restart_msg, restart_rect)
        pygame.display.flip()
    def run(self):
        """
        Main game loop.
        """
