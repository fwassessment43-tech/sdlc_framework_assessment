'''
Main entry point for the snake game.
'''

import pygame
from snake import Snake
from food import Food
from board import Board
from clock import Clock

def main():
    # Initialize Pygame
    pygame.init()

    # Set up display dimensions
    width, height = 800, 600
    screen = pygame.display.set_mode((width, height))

    # Set up title
    pygame.display.set_caption('Snake Game')

    # Create game objects
    snake = Snake(100, 50)
    food = Food(width // 2, height // 2)
    board = Board(width, height)
    clock = Clock()

    # Main game loop
    running = True
    while running:
        for event in pygame.event.get():
            if event.type == pygame.QUIT:
                running = False

        # Handle snake movement
        snake.move(board.get_snake_body(), clock.get_time())

        # Generate new food
        food.generate_new_food()

        # Check collisions
        snake.check_collisions(food, board)

        # Draw everything
        screen.fill((0, 0, 0))
        board.draw(screen)
        snake.draw(screen)
        food.draw(screen)

        # Update display
        pygame.display.flip()
        clock.tick(10)

    # Quit Pygame
    pygame.quit()

if __name__ == '__main__':
    main()
```
