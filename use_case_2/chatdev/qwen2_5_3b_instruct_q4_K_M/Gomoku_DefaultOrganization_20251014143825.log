                "oss:PutObject"
            ],
            "Resource": "*",
            "Condition": {
                "IpAddress": [{"aws:SourceIp": ["192.0.2.0/24"]}]
            }
        },
        {
            "Sid": "DenyNonDevOpsAccessAndOthers",
            "Effect": "Deny",
            "Principal": {"AWS": "*"},
            "Action": [
                "oss:ListBucket"
            ],
            "Resource": "*",
            "Condition": {
                "IpAddress": [{"aws:SourceIp": ["192.0.2.0/24"]}]
