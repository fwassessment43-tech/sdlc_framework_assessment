import tkinter as tk
from PIL import ImageTk, Image
def get_icon():
    # Load the icon from the file
    img = Image.open("imageviewer.ico")
    # Convert the image to a Tk photo image
    img = ImageTk.PhotoImage(img)
    # Create a Tk label with the photo image
    lbl_icon = tk.Label(image=img)
    lbl_icon.pack()
    return lbl_icon
if __name__ == "__main__":
    root = tk.Tk()
    lbl_icon = get_icon()
    root.mainloop()